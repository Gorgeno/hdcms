    "sass": "^1.20.1",
    "sass-loader": "^8.0.0",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "dependencies": {
    "@toast-ui/editor": "^2.2.0",
