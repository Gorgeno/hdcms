{
  "name": "houdunwang/hdcms",
  "type": "project",
  "description": "模块化多应用平台系统.",
  "keywords": [
    "hdcms",
    "houdunren",
    "wechat"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2.5",
    "alibabacloud/client": "^1.5",
    "aliyuncs/oss-sdk-php": "^2.3",
    "caouecs/laravel-lang": "~6.0",
    "endroid/qr-code": "^3.9",
    "erusev/parsedown": "^1.7",
    "fideloper/proxy": "^4.2",
    "fruitcake/laravel-cors": "^2.0",
    "graham-campbell/markdown": "^13.0",
    "guzzlehttp/guzzle": "^6.3",
    "hieu-le/active": "^7.0",
    "hisorange/browser-detect": "^4.2",
    "houdunwang/arr": "^2.0",
    "laravel/framework": "^7.0",
    "laravel/socialite": "^4.4",
    "laravel/telescope": "^3.5",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^2.0",
    "livewire/livewire": "^1.3",
    "mews/captcha": "^3.1",
    "mews/purifier": "^3.3",
    "nwidart/laravel-modules": "^7.1",
    "overtrue/easy-sms": "^1.1",
    "socialiteproviders/wechat-web": "^1.0",
    "socialiteproviders/weixin": "^4.0",
    "socialiteproviders/weixin-web": "^3.1",
    "spatie/image": "^1.7",
    "spatie/laravel-activitylog": "^3.14",
    "spatie/laravel-permission": "^3.13",
    "workerman/gateway-worker": "^3.0",
    "workerman/gatewayclient": "^3.0",
    "yansongda/pay": "^2.9"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.3",
    "barryvdh/laravel-ide-helper": "^2.7",
    "facade/ignition": "^2.0",
    "fzaninotto/faker": "^1.9.1",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^4.1",
    "phpunit/phpunit": "^8.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Modules\\": "Modules/",
      "Socket\\": "socket/",
      "Houdunwang\\WeChat\\": "wechat/src/"
    },
    "files": [
      "app/helpers.php"
    ],
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
